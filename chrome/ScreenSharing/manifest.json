{
  "name" : "Potluck Screen Sharing",
  "author": "Anthony Harley",
  "version" : "1.0",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "This Chrome extension is developed primary for https://potluck.chat/. It doesn't do anything except capture content of your screen.",
  "homepage_url": "https://potluck.chat/",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://staging.potluck.chat/*",
      "https://potluck.chat/*",
      "https://dev.potluck:4443/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
